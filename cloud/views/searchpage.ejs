					          	<div class="col-sm-8 blog-main">
									<div id="searchresults"></div>
									<input type="submit" id="loadMoreBtn" value="Load more" class="tfbutton" onclick="loadMore();" id="loadMoreBtn">
							   	</div><!-- /.blog-main -->
						    </div><!-- /.row -->
						</div><!-- /.container -->
				  </div>
		</div>		  		
		

