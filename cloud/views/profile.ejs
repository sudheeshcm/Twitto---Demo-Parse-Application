  <div>	
	    <div class="blog-masthead">
	      <div class="container">
	        <nav class="blog-nav">
	          <a class="blog-nav-item" href="/homepage" id="loggeduser"></a>
	          <span style="float: center; padding: 4px; padding-left: 385px;">	  
		      		<input type="text" class="tftextinput" id="keyword" name="keyword" size="21" maxlength="120">
		      		<input type="image" id="myimage" style="height: 35px; width: 40px; vertical-align: middle;" src="/css/Images/search-button.png" alt="Search" onclick="userSearch();"/>
		      </span>
